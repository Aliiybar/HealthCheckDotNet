{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Products",
        "Uri": "http://host.docker.internal:8022/health"
      },
      {
        "Name": "Sign Api",
        "Uri": "http://host.docker.internal:8011/health"
      }
    ],
    "EvaluationTimeInSeconds": 5,
    "MinimumSecondsBetweenFailureNotification":  60
  },
  "AllowedHosts": "*"
}
